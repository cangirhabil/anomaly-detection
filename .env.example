# Environment variables için örnek dosya
# .env olarak kopyalayın ve değerleri düzenleyin

# Servis Portu
PORT=8000

# Anomali Tespit Ayarları
ANOMALY_WINDOW_SIZE=30
ANOMALY_Z_THRESHOLD=2.0
ANOMALY_MIN_POINTS=7

# Logging
LOG_LEVEL=INFO

# CORS (virgülle ayrılmış)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080
